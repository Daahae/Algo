    "N = int(input())\n",
    "\n",
    "lst = []\n",
    "\n",
    "for _ in range(N):\n",
    "    lst.append(int(input()))\n",
    "    \n",
    "for i in lst:\n",
    "    cnt = 0\n",
    "    # 3을 한번씩 빼주기\n",
    "    for tmp_num in range(0, (i // 3)+1):\n",
    "        # 2로 나눈 몫 + 1\n",
    "        cnt += ((i - (3 * tmp_num)) // 2) + 1\n",
    "    \n",
    "    print(cnt)"
   ]
  }
